<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>数据导入</title>
</head>
<body>
	<h3>段位文档上传</h3>
	<form action="${pageContext.request.contextPath}/excel/gradeFileUp"
		method="post" enctype="multipart/form-data">
		<label>文档:</label> <input type="file" name="file"> <input
			type="submit" value="submit">
	</form>
	<br>
	<hr>
	<br>
	<h3>指标数据汇总上传</h3>
	<form action="${pageContext.request.contextPath}/excel/dataFileUp"
		method="post" enctype="multipart/form-data">
		<label>文档:</label> <input type="file" name="file"> <input
			type="submit" value="submit">
	</form>
	<hr>
	<br>
	<h3>值表文档上传</h3>
	<form action="${pageContext.request.contextPath}/excel/valueFileUp"
		method="post" enctype="multipart/form-data">
		<label>文档:</label> <input type="file" name="file"> <input
			type="submit" value="submit">
	</form>
	<h3>营销部文档导入</h3>
	<form action="${pageContext.request.contextPath}/precision/precisionFile"
		method="post" enctype="multipart/form-data">
		<label>文档:</label> <input type="file" name="file"> <input
			type="submit" value="submit">
	</form>
	<br>
	<hr>
	<br>
	<a href="${pageContext.request.contextPath}/excel/a">ddd</a>
</body>
</html>