package com.womow.henan.modules.sys.security.dto;

import java.util.List;

import com.womow.henan.modules.sys.security.po.BusRoleEntity;

public class BusRoleEntityDo extends BusRoleEntity{

	
	
	private Integer[] authIds; //权限ID
	
	private List<String> descriptions; //权限标识描述集合
	
	private List<BusAuthorityEntityDo> auths; //权限
	
	public List<String> getDescriptions() {
		return descriptions;
	}

	public void setDescriptions(List<String> descriptions) {
		this.descriptions = descriptions;
	}

	public Integer[] getAuthIds() {
		return authIds;
	}

	public void setAuthIds(Integer[] authIds) {
		this.authIds = authIds;
	}

	public List<BusAuthorityEntityDo> getAuths() {
		return auths;
	}

	public void setAuths(List<BusAuthorityEntityDo> auths) {
		this.auths = auths;
	}
}
