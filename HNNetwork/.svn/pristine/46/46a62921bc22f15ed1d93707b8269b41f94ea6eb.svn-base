package com.womow.henan.modules.province.bean.po;

import com.womow.henan.commons.bean.BaseEntity;

/**
 * @Description 精益化指标意见实体
 * @author CAI modules@163.com
 * @data 2017年9月18日上午10:59:32
 * @version V1.0
 */
public class BusPrecisionQuestionEntity extends BaseEntity{
	private int year;
	private int month;
	private String quotaName; //精益化指标名称
	private String monitorDate; //监控日期
	private String questionType; //问题分类
	private String dutyUnit; //责任单位
	private String questionReason; //问题原因
	private String cureOpinion; //整改意见
	private String cureTerm; //整改期限
	private String cureResult; //整改结果
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public String getQuotaName() {
		return quotaName;
	}
	public void setQuotaName(String quotaName) {
		this.quotaName = quotaName;
	}
	public String getMonitorDate() {
		return monitorDate;
	}
	public void setMonitorDate(String monitorDate) {
		this.monitorDate = monitorDate;
	}
	public String getQuestionType() {
		return questionType;
	}
	public void setQuestionType(String questionType) {
		this.questionType = questionType;
	}
	public String getDutyUnit() {
		return dutyUnit;
	}
	public void setDutyUnit(String dutyUnit) {
		this.dutyUnit = dutyUnit;
	}
	public String getQuestionReason() {
		return questionReason;
	}
	public void setQuestionReason(String questionReason) {
		this.questionReason = questionReason;
	}
	public String getCureOpinion() {
		return cureOpinion;
	}
	public void setCureOpinion(String cureOpinion) {
		this.cureOpinion = cureOpinion;
	}
	public String getCureTerm() {
		return cureTerm;
	}
	public void setCureTerm(String cureTerm) {
		this.cureTerm = cureTerm;
	}
	public String getCureResult() {
		return cureResult;
	}
	public void setCureResult(String cureResult) {
		this.cureResult = cureResult;
	}
}
